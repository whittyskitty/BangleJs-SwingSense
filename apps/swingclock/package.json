{
  "name": "swingclock",
  "version": "0.1.0",
  "description": "A Bangle.js app for swing analysis",
  "main": "app.js",
  "scripts": {
    "version:patch": "node version-bump.js patch",
    "version:minor": "node version-bump.js minor",
    "version:major": "node version-bump.js major",
    "version:show": "node -e \"const fs=require('fs');const content=fs.readFileSync('app.js','utf8');const match=content.match(/version:\\s*['\"]([^'\"]+)['\"]/);console.log(match?match[1]:'unknown');\"",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": ["banglejs", "golf", "swing", "tracking"],
  "author": "Scott Whitaker",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  }
} 